#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/f/docs-demo/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_3f202e2aa17f5bb383874f855d576800/node_modules/vitepress/bin/node_modules:/mnt/f/docs-demo/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_3f202e2aa17f5bb383874f855d576800/node_modules/vitepress/node_modules:/mnt/f/docs-demo/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_3f202e2aa17f5bb383874f855d576800/node_modules:/mnt/f/docs-demo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/f/docs-demo/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_3f202e2aa17f5bb383874f855d576800/node_modules/vitepress/bin/node_modules:/mnt/f/docs-demo/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_3f202e2aa17f5bb383874f855d576800/node_modules/vitepress/node_modules:/mnt/f/docs-demo/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_3f202e2aa17f5bb383874f855d576800/node_modules:/mnt/f/docs-demo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/vitepress@1.6.4_@algolia+cl_3f202e2aa17f5bb383874f855d576800/node_modules/vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../.pnpm/vitepress@1.6.4_@algolia+cl_3f202e2aa17f5bb383874f855d576800/node_modules/vitepress/bin/vitepress.js" "$@"
fi
